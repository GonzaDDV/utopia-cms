[uwsgi]
single-interpreter=true
chdir=/srv/utopia-cms
module=wsgi:application
master=false
socket=/var/local/utopia-cms.sock
http11-socket=true
pidfile=/var/local/utopia-cms.pid
vacuum=true
max-requests=8000
processes=4
queue=4096
listen=4096
home=/opt/venv/utopiacms
daemonize=/var/log/uwsgi/utopia-cms.log
max-worker-lifetime=360
